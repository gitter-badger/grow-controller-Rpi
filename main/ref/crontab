###INSTALL AS ROOT###
@reboot python3 /home/pi/grow-controller-Rpi/main/ref/main.py &  >> /home/pi/grow-controller-Rpi/main/ref/error.log
@reboot python3 /home/pi/grow-controller-Rpi/main/ref/boot.py &
@reboot sudo echo 27 > /sys/class/gpio/export
@reboot sudo echo 22 > /sys/class/gpio/export
@reboot sudo echo 18 > /sys/class/gpio/export
@reboot sudo echo 23 > /sys/class/gpio/export
0 8 * * * python3 /home/pi/grow-controller-Rpi/main/ref/relay.py -m on
0 20 * * * python3 /home/pi/grow-controller-Rpi/main/ref/relay.py -m off
0 8 * * * python3 /home/pi/grow-controller-Rpi/main/ref/relay.py -b on
0 20 * * * python3 /home/pi/grow-controller-Rpi/main/ref/relay.py -b off
*/5 * * * * python3 /home/pi/grow-controller-Rpi/main/ref/IOupdater.py >> /home/pi/grow-controller-Rpi/main/ref/error.log
